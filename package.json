{
  "name": "react-three-renderer-starter",
  "version": "0.1.0",
  "description": "A basic webpack setup for a static site using react-three-renderer.",
  "main": "src/index.js",
  "scripts": {
    "watch": "./node_modules/webpack-dev-server/bin/webpack-dev-server.js --watch --hot --inline --progress --config webpack.dev.config.js",
