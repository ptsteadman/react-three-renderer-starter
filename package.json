    "url": "https://github.com/ptsteadman/react-three-renderer-starter/issues"
  },
  "homepage": "https://github.com/ptsteadman/react-three-renderer-starter#readme",
  "dependencies": {
    "react": "~15.1.0",
    "react-dom": "~15.1.0",
    "react-highlight": "^0.8.0",
    "react-router": "^2.5.2",
    "react-three-renderer": "^2.1.3",
    "three": "0.77.0"
  },
  "devDependencies": {
    "babel-core": "^6.10.4",
